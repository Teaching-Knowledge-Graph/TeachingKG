                           rdfs:label "consists of knowledge" .


###  https://github.com/tibonto/educor#requiresKnowledge
educor:requiresKnowledge rdf:type owl:ObjectProperty ;
                         rdfs:domain educor:Skill ;
                         rdfs:range educor:KnowledgeTopic ;
                         rdfs:label "requires knowledge" .


###  https://w3id.org/def/courses#competencyRequired
courses:competencyRequired rdf:type owl:ObjectProperty ;
                           rdfs:domain educor:Skill ;
                           rdfs:range educor:KnowledgeTopic ,
                                      educor:Skill ;
                           rdfs:label "competency required" .


###  https://w3id.org/def/courses#containsResource
courses:containsResource rdf:type owl:ObjectProperty ;
                         rdfs:domain courses:CourseSession ;
                         rdfs:range educor:EducationalResource ;
                         rdfs:label "contains resource" .


###  https://w3id.org/def/courses#coursePrerequisites
courses:coursePrerequisites rdf:type owl:ObjectProperty ;
                            rdfs:domain schema:Course ;
                            rdfs:label "course prerequisites" .


###  https://w3id.org/def/courses#hasType
courses:hasType rdf:type owl:ObjectProperty ;
                rdfs:domain educor:EducationalResource ;
                rdfs:range [ owl:intersectionOf ( skos:Concept
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty skos:inScheme ;
                                                    owl:hasValue <https://w3id.org/coursesonto/kos/educational-type>
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
                rdfs:label "has type" .


###  https://w3id.org/def/courses#responsibleEntity
courses:responsibleEntity rdf:type owl:ObjectProperty ;
                          rdfs:domain schema:Course ;
                          rdfs:label "responsible entity" .


